    "native-base": "^2.13.14",
    "npm": "^6.14.8",
    "prop-types": "^15.7.2",
    "re-base": "^4.0.0",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-native": "0.63.4",
    "react-native-audio": "^4.3.0",
    "react-native-audio-recorder-player": "^2.6.0",
    "react-native-fast-image": "^8.3.4",
    "react-native-fbsdk": "^3.0.0",
